{
  "permissions": {
    "function": {
      "deviceSimulatorFn": [
        "read"
      ]
    },
    "storage": {
      "Itinerary:@model(appsync)": [
        "read",
        "update"
      ]
    }
  },
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "awssupplychaindemopowertools",
      "env": "dev",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true
    }
  ],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "powertoolsServiceName",
      "environmentVariableName": "POWERTOOLS_SERVICE_NAME"
    }
  ]
}